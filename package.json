{
  "name": "my-libp2p-sdk",
  "version": "0.0.1",
  "description": "A high-end libp2p sdk for p2p project",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch ./src/app.ts",
    "start": "bun ./src/app.ts",
    "type-docs": "typedoc",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@vitest/coverage-v8": "4.0.8",
    "typedoc": "^0.28.14",
    "vitest": "^4.0.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "typedocOptions": {
    "entryPointStrategy": "expand",
    "entryPoints": ["src"],
    "out": "docs/api",
    "theme": "default",
    "includeVersion": true,
    "hideGenerator": true
  }
}
